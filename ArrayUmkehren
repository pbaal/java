import java.util.*;

public class ArrayUmkehren
{
  static void printArray(int[] a)
  {
    int[] array = {1,2,3,4,5};
    for (int i = 0; i < array.length; i++);
    {
    if(i>0) Out.print(", ");
      Out.println(array[i]);
    }
  }

  //Teil a)
  static int[] reverse(int[] a)
  {
    int[] b; 
    b = new int[a.length];
    for(int i = 0; i <= a.length; i++)
    {
      for(int j = a.length; j >= 0; j--)
      {
        b[j] = a[i];
      }
    }
    return b;
  }



}
